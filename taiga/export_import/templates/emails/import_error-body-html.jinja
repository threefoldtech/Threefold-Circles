{% extends "emails/base-body-html.jinja" %}

{% block body %}
    {% trans user=user.get_full_name(), error_message=error_message, support_url=sr("support.url") %}
    <h1>{{ error_message }}</h1>
    <p>Hello {{ user }},</p>
    <p>Your project has not been importer correctly.</p>
    <p>The ThreeFold Circles system administrators have been informed.<br/> Please, try it again or contact with the support team at
       <a href="{{ support_url }}" title="Support" style="color: #699b05">{{ support_url }}</a></p>
    <p><small>The ThreeFold Circles Team</small></p>
    {% endtrans %}
{% endblock %}
